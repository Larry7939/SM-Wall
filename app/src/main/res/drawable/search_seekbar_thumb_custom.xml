<?xml version="1.0" encoding="utf-8"?>
<!-- 원모양의 seekbar thumb -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"
    android:useLevel="false"  >
    <!-- 배경 색상 -->
    <solid
        android:color="#FF857C"/>
    <!-- 테두리 색상 (생략 가능) -->
    <stroke
        android:width="2dp"
        android:color="#FF3F31" />
    <!-- 크기 -->
    <size
        android:width="26dp"
        android:height="26dp"/>
</shape>